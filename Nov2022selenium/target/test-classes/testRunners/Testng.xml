<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Suite" verbose="4">

	<test name="Cart Test">
	
		<parameter name="url" value="https://naveenautomationlabs.com/opencart/" />
		<parameter name="browser" value="chrome" />


		<classes>
			<class name="testngSession.CartTest" />
		</classes>
	</test>

	<test name="Login Test">
	
		<parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/login" />
		<parameter name="browser" value="firefox" />
		
		<classes>
			<class name="testngSession.LoginTest" />
		</classes>
	</test>

	<test name="Register Test">
	
		<parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/register" />
		<parameter name="browser" value="chrome" />

		<classes>
			<class name="testngSession.RegisterTest" />
		</classes>
	</test>


</suite>